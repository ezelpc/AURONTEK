    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^6.4.17",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}