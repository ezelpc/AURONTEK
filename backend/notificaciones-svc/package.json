{
  "name": "notificaciones-svc",
  "version": "1.0.0",
  "description": "Servicio de Notificaciones por email",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "clean": "rimraf dist",
