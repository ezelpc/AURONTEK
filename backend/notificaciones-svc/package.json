{
  "name": "notificaciones-svc",
  "version": "1.0.0",
  "description": "Servicio de Notificaciones por email",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon index.js",
    "clean": "rimraf dist",
    "build": "npm run clean && babel . -d dist --extensions \".js\" --ignore \"node_modules\",\"dist\"",
    "start": "node dist/index.js",
    "cache": "rimraf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.0"
  }
}