{
  "name": "tickets-svc",
  "version": "1.0.0",
  "description": "Servicio de Tickets para la plataforma de Help Desk",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon index.js",
    "clean": "rimraf dist",
    "build": "npm run clean && babel . -d dist --extensions \".js\" --ignore \"node_modules\",\"dist\"",
    "start": "node dist/index.js",
    "cache": "rimraf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "amqplib": "^0.10.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.0"
  }
}